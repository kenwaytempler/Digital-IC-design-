#include <stack>
#include <vector>
bool validateStackOrder(std::vector<int>& pushed, std::vector<int>& popped) {
    std::stack<int> s;
    int i = 0;

    for (auto x : pushed) {
        s.push(x);
        while (!s.empty() && s.top() == popped[i]) {
            s.pop();
            i++;
        }
    }

    return s.empty();
}